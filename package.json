{
  "name": "@self-denial-cy/vue-super-tree",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --dest lib --target lib --name vue-super-tree ./src/index.js",
    "postbuild:lib": "rm ./lib/demo.html",
    "analyze": "vue-cli-service build --report --dest lib --target lib --name vue-super-tree ./src/index.js",
    "postanalyze": "rm ./lib/demo.html",
    "git:auto": "git add . && git commit -m 'some update' && git push"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  }
}