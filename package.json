{
  "name": "@self-denial-cy/vue-super-tree",
  "version": "1.0.1",
  "description": "a tree component based on vue@2, which supports mass tree node and some attributes",
  "files": [
    "lib"
  ],
  "main": "lib/vue-super-tree.umd.min.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --dest lib --target lib --name vue-super-tree ./src/index.js",
    "postbuild:lib": "rm ./lib/demo.html",
    "analyze": "vue-cli-service build --report --dest lib --target lib --name vue-super-tree ./src/index.js",
    "postanalyze": "rm ./lib/demo.html",
    "git:auto": "git add . && git commit -m 'some update' && git push && git push GitHub"
  },
  "keywords": [
    "vue@2",
    "tree",
    "mass tree node",
    "vue-super-tree",
    "vue super tree"
  ],
  "author": "self-denial-cy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/self-denial-cy/vue-super-tree.git"
  },
  "bugs": "https://gitee.com/self-denial-cy/vue-super-tree/issues",
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.9.7"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  }
}